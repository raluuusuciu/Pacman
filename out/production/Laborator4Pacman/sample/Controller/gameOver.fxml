<?xml version="1.0" encoding="UTF-8"?>

<?import java.net.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.paint.*?>
<?import javafx.scene.text.*?>

<AnchorPane fx:id="gameOver" prefHeight="398.0" prefWidth="634.0" xmlns:fx="http://javafx.com/fxml/1" xmlns="http://javafx.com/javafx/2.2" fx:controller="sample.Controller.GameOverController">
  <children>
    <Label alignment="CENTER" layoutX="31.0" prefHeight="118.0" prefWidth="572.0" text="GAME OVER!" textFill="#fff500">
      <font>
        <Font name="Century Gothic Bold" size="50.0" />
      </font>
    </Label>
    <TableView fx:id="tableScore" layoutX="14.0" layoutY="118.0" prefHeight="175.0" prefWidth="607.0">
      <columns>
        <TableColumn prefWidth="217.0" text="Username" fx:id="usernameColumn" />
        <TableColumn prefWidth="201.0" text="Score" fx:id="scoreColumn" />
        <TableColumn prefWidth="185.0" text="Date" fx:id="dateColumn" />
      </columns>
    </TableView>
    <Label layoutX="31.0" layoutY="315.0" prefWidth="286.0" text="Do you want to join for future progress?">
      <font>
        <Font name="Century Gothic Bold" size="15.0" fx:id="x2" />
      </font>
      <textFill>
        <Color green="0.960" red="1.000" fx:id="x1" />
      </textFill>
    </Label>
    <TextField fx:id="enrollMe" layoutX="411.0" layoutY="308.0" prefWidth="200.0" promptText="Enter Username" />
    <Button fx:id="viewButton" layoutX="553.0" layoutY="358.0" mnemonicParsing="false" onAction="#viewLeaderBoard" text="View" />
    <Button fx:id="enrollUser" layoutX="452.0" layoutY="358.0" mnemonicParsing="false" onAction="#addUser" text="Enroll Me" />
    <Label layoutX="14.0" layoutY="86.0" text="LEADERBOARD" textFill="$x1">
      <font>
        <Font name="Century Gothic Bold" size="20.0" />
      </font>
    </Label>
    <Label fx:id="message" font="$x2" layoutX="31.0" layoutY="361.0" prefWidth="330.0" textFill="$x1" />
  </children>
  <stylesheets>
    <URL value="@../Stuff/custom.css" />
  </stylesheets>
</AnchorPane>
